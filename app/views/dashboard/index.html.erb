
<%=link_to "Nowy news", new_post_path%><br>
<% @posts.each do |f| %>
<%=f.title%><br>
<%=f.content%><br>
<%=link_to "Czytaj dalej...", post_path(f.id)%> <br>
<%=link_to "Edytuj news", edit_post_path(f.id)%>
<%=link_to "Usuń news", post_path(f.id), method: "delete"%><br><br>
<%end%>

<% @partners.each do |f| %>
	<%= link_to(image_tag(f.photo),"#{f.link}")%> 
	<%=form_for f, method: "patch", :html => {:multipart => true}  do |t|%>
		<%= t.label :upload %><br>
		<%= t.file_field :photo %><br>
		<%=t.submit%>
	<%end%>
	<%=link_to "Usuń partnera", partner_path(f.id), method: "delete"%><br><br>
<%end%>

<%=form_for Partner.new, :html => {:multipart => true} do |f|%>
	<%= f.label :upload %><br>
	<%= f.file_field :photo %><br>
	<%=f.submit%>
<%end%>

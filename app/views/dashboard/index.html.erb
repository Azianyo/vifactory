
<%=link_to "Nowy news", new_post_path%><br>
<%= render @posts %>
<%= will_paginate @posts %>

<% @partners.each do |partner| %>
	<%= link_to(image_tag(partner.photo),"#{partner.link}")%> 
	<%=form_for partner, method: "patch", :html => {:multipart => true}  do |t|%>
		<%= t.label :upload %><br>
		<%= t.file_field :photo %><br>
		<%=t.submit%>
	<%end%>
	<%=link_to "UsuÅ„ partnera", partner_path(partner), method: "delete"%><br><br>
<%end%>

<%=form_for Partner.new, :html => {:multipart => true} do |f|%>
	<%= f.label :upload %><br>
	<%= f.file_field :photo %><br>
	<%=f.submit%>
<%end%>
